        "type": "numeric",
        "regex": "^[0-9]{3}$",
        "required": true,
        "info": "Subs\u00e9rie do documento fiscal",
        "format": ""
    },
    "COD_CONS": {
        "type": "string",
        "regex": "^0[1|2|3|4|5|6|7|8]$",
        "required": true,
        "info": "Co\u0301digo de classe de consumo de energia ele\u0301trica: 01 - Comercial 02 - Consumo Pro\u0301prio 03 - Iluminac\u0327a\u0303o Pu\u0301blica 04 - Industrial 05 - Poder Pu\u0301blico 06 - Residencial 07 - Rural 08 -Servic\u0327o Pu\u0301blico",
        "format": ""
    },
    "QTD_CONS": {
        "type": "numeric",
