{
    "IND_OPER": {
        "type": "string",
        "regex": "^.{1}$",
        "required": true,
        "info": "Indicador do tipo de operac\u0327a\u0303o: 1- Sai\u0301da",
        "format": ""
    },
    "IND_EMIT": {
        "type": "string",
