        "format": ""
    },
    "VL_DOC": {
        "type": "numeric",
        "regex": "^\\d+(\\.\\d*)?|\\.\\d+$",
        "required": true,
        "info": "Valor total do documento fiscal",
        "format": "15v2"
    },
    "VL_DESC": {
        "type": "numeric",
